{
  "repo": "owner/repo-name",
  "include_readme": true,
  "include_docs": true,
  "include_code": false,
  "include_issues": false,
  "include_releases": true,
  "code_analysis_depth": "surface",
  "max_files": 100,
  "file_patterns": {
    "include": ["*.md", "*.py", "*.ts", "*.js", "*.tsx", "*.jsx"],
    "exclude": ["*.test.*", "*.spec.*", "node_modules/*", "__pycache__/*"]
  }
}
