{
  "name": "example-framework",
  "description": "Example framework documentation skill",
  "base_url": "https://docs.example.com/",
  "selectors": {
    "main_content": "article, main, .content, [role='main']",
    "title": "h1",
    "code_blocks": "pre code, pre"
  },
  "url_patterns": {
    "include": ["/docs", "/guide", "/api", "/reference"],
    "exclude": ["/blog", "/changelog", "/about", "/community"]
  },
  "categories": {
    "getting_started": ["intro", "quickstart", "installation", "setup"],
    "core_concepts": ["concepts", "fundamentals", "basics"],
    "api_reference": ["api", "reference", "methods"],
    "guides": ["guide", "tutorial", "how-to"],
    "examples": ["example", "sample", "demo"]
  },
  "rate_limit": 0.5,
  "max_pages": 200,
  "timeout": 30
}
